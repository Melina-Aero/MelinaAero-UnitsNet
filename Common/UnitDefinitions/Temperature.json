{
  "Name": "Temperature",
  "BaseUnit": "Kelvin",
  "XmlDoc": "A temperature is a numerical measure of hot or cold. Its measurement is by detection of heat radiation or particle velocity or kinetic energy, or by the bulk behavior of a thermometric material. It may be calibrated in any of various temperature scales, Celsius, Fahrenheit, Kelvin, etc. The fundamental physical definition of temperature is provided by thermodynamics.",
  "GenerateArithmetic": false,
  "BaseDimensions": {
    "Θ": 1
  },
  "Units": [
    {
      "SingularName": "Kelvin",
      "PluralName": "Kelvins",
      "FromUnitToBaseFunc": "x",
      "FromBaseToUnitFunc": "x",
      "Localization": [
        {
          "Culture": "en-US",
          "Abbreviations": [ "K" ]
        }
      ]
    },
    {
      "SingularName": "DegreeCelsius",
      "PluralName": "DegreesCelsius",
      "FromUnitToBaseFunc": "x + 273.15",
      "FromBaseToUnitFunc": "x - 273.15",
      "Localization": [
        {
          "Culture": "en-US",
          "Abbreviations": [ "°C" ]
        }
      ]
    },
    {
      "SingularName": "DegreeFahrenheit",
      "PluralName": "DegreesFahrenheit",
      "FromUnitToBaseFunc": "x*5/9 + 459.67*5/9",
      "FromBaseToUnitFunc": "(x - 459.67*5/9)*9/5",
      "Localization": [
        {
          "Culture": "en-US",
          "Abbreviations": [ "°F" ]
        }
      ]
    },
    {
      "SingularName": "DegreeNewton",
      "PluralName": "DegreesNewton",
      "FromUnitToBaseFunc": "x*100/33 + 273.15",
      "FromBaseToUnitFunc": "(x - 273.15)*33/100",
      "Localization": [
        {
          "Culture": "en-US",
          "Abbreviations": [ "°N" ]
        }
      ]
    },
    {
      "SingularName": "DegreeRankine",
      "PluralName": "DegreesRankine",
      "FromUnitToBaseFunc": "x*5/9",
      "FromBaseToUnitFunc": "x*9/5",
      "Localization": [
        {
          "Culture": "en-US",
          "Abbreviations": [ "°R" ]
        }
      ]
    }
  ]
}